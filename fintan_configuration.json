{
  "input": null,
  "output": null,
  "pipeline": null,
  "components": [
    {
      "componentInstance": "lexicalEntriesExportFromSQL",
      "class": "SQLStreamTransformer",
      "driver": null,
      "connectUrl": "jdbc:mysql://localhost:3306/lexisdb",
      "user": "root",
      "password": "<$param0>",
      "query": "scripts/exportLexicalEntries.sql",
      "outFormat": "TSV",
      //the following will be ignored, since outFormat is defined.
      "escapeChar": null,
      "delimiterCSV": "\t",
      "quoteChar": null,
      "emptyChar": "_"
    },		
    {
      "componentInstance": "lexicalEntriesLoadFromSQL",
      "class": "RDFStreamLoader",
      "lang": "Turtle",
      "globalPrefixes": true,
      "delimiter": null,
      "split": false
    },
    {
      "componentInstance": "lexicalEntriesWriteToFile",
      "class": "RDFStreamWriter",
      "lang": "Turtle"
    }
  ],
  "streams": [
    // Stream 1
		// Read Lexical entry from SQL db and load to Fintan
		{
			"readsFromInstance": "lexicalEntriesExportFromSQL",
			"writesToInstance": "lexicalEntriesLoadFromSQL",
			"writesToInstanceGraph": "lexicalEntries"
		},		
		// Export TSV results from db export
    {
      "readsFromInstance": "lexicalEntriesLoadFromSQL",
      "readsFromInstanceGraph": "lexicalEntries", 
      "writesToInstance": "lexicalEntriesWriteToFile",
      "writesToInstanceGraph": ""
    },
    {
      "readsFromInstance": "lexicalEntriesWriteToFile",
      "readFromInstanceGraph": "",
      "writesToDestination": "LexisFinalRDF/exportedLexicalEntriesMapping.ttl"
    }
  ]
}
