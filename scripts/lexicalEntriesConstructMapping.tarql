PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX lexinfo: <http://www.lexinfo.net/ontology/3.0/lexinfo#>
PREFIX ontolex: <http://www.w3.org/ns/lemon/ontolex#>
PREFIX lime: <http://www.w3.org/ns/lemon/lime#>
PREFIX conll: <http://ufal.mff.cuni.cz/conll2009-st/task-description.html#>
PREFIX apertium: <http://wiki.apertium.org/wiki/Bidix#>
PREFIX lexis: <http://www.exaple.com/lexis#>

CONSTRUCT {
  ?le_uri rdf:type ontolex:LexicalEntry;
          rdfs:label ?labelWithLang ;
          lexis:partOfSpeech ?c;
          lexinfo:partOfSpeech ?c
  .
}
WHERE {
  BIND(URI(CONCAT('http://www.exaple.com/lexis/LexicalEntry', ?a)) as ?le_uri) 
  BIND (STRLANG(?b, "el") AS ?labelWithLang)
}
